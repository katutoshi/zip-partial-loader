{
  "name": "zip-partial-loader",
  "version": "0.10.0",
  "description": "A library for partial ZIP file loading using Range Requests",
  "main": "dist/lszl.js",
  "types": "dist/lszl.d.ts",
  "scripts": {
    "dev": "webpack serve --open",
    "clean": "rm -rf dist lib && cd wasm && rm -rf pkg && cargo clean",
    "build": "tsc && webpack --config webpack.config.lszl.js --mode=${NODE_ENV:-development}",
    "prepack": "$npm_execpath run build",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "author": "katutoshi",
  "contributors": [
    "Lunascape (original author)"
  ],
  "license": "MIT",
  "devDependencies": {
    "copy-webpack-plugin": "^13.0.1",
    "esbuild-loader": "^4.4.2",
    "mime": "^4.1.0",
    "msw": "^2.12.8",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3"
  },
  "files": [
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "dist/lszl.d.ts",
    "dist/lszl.js",
    "dist/lszlw.js"
  ]
}
